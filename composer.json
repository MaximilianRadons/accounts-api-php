{
    "name": "getmyinvoices/accounts-api-php",
    "description": "A PHP SDK for the GetMyInvoices Accounts API",
    "version": "1.0.0",
    "keywords": ["getmyinvoices","invoices","api"],
    "homepage": "http://github.com/getmyinvoices/accounts-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "GetMyInvoices SDK Team",
            "homepage": "https://api.getmyinvoices.com/accounts/v2/doc/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://github.com/getmyinvoices/accounts-api-php/issues",
        "source": "http://github.com/getmyinvoices/accounts-api-php"
    },
    "require": {
        "guzzlehttp/guzzle": "^6.0",
        "ext-json": "*"
    },
    "autoload": {
        "classmap": ["src/"],
